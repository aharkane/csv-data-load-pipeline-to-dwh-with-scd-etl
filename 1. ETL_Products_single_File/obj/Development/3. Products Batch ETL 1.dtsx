      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{1B9D892C-DC30-46EF-8F0C-5AC0F2DB5797}"
          SQLTask:SqlStatementSource="&#xA;use AdvWrk&#xA;go&#xA;&#xA;drop table if exists stg.temp &#xA;go &#xA;select&#xA;&#xA;&#x9;t.ProductID &#xA;&#x9;,case &#xA;&#x9;&#x9;when t.ProductID = s.ProductID and t.UnitPrice = s.UnitPrice &#xA;&#x9;&#x9;&#x9;and t.ProductName &lt;&gt; s.ProductName and s.ProductName is not null &#xA;&#x9;&#x9;then s.ProductName&#xA;&#x9;&#x9;else t.ProductName &#xA;&#x9;end as ProductName&#xA;&#x9;,t.SupplierID&#xA;&#x9;,t.CategoryID&#xA;&#x9;,t.QuantityPerUnit&#xA;&#x9;,t.UnitPrice&#xA;&#x9;,t.UnitsInStock&#xA;&#x9;,t.UnitsOnOrder&#xA;&#x9;,t.ReorderLevel&#xA;&#x9;,t.Discontinued&#xA;&#x9;, Case &#xA;&#x9;&#x9;When t.ProductID = s.ProductID and t.UnitPrice &lt;&gt; s.UnitPrice and s.UnitPrice is not null &#xA;&#x9;&#x9;then 0&#xA;&#x9;&#x9;else 1 &#xA;&#x9;end as IsCurrent &#xA;into stg.temp &#xA;from test.products t left join stg.products s&#xA;on t.ProductID = s.ProductID and t.IsCurrent = 1&#xA;union all&#xA;select&#xA;&#x9;t.ProductID &#xA;&#x9;,s.ProductName &#xA;&#x9;,t.SupplierID&#xA;&#x9;,t.CategoryID&#xA;&#x9;,t.QuantityPerUnit&#xA;&#x9;,s.UnitPrice&#xA;&#x9;,t.UnitsInStock&#xA;&#x9;,t.UnitsOnOrder&#xA;&#x9;,t.ReorderLevel&#xA;&#x9;,t.Discontinued&#xA;&#x9;,s.IsCurrent &#xA;&#xA;from test.products t inner join stg.products s&#xA;on t.ProductID = s.ProductID and t.IsCurrent = 1&#xA;where s.ProductID is not null&#xA;&#xA;go &#xA;&#xA;truncate table test.products&#xA;go&#xA;&#xA;insert into test.products select * from stg.temp &#xA;go&#xA;&#xA;select*  from test.products" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
